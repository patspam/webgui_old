          &nbsp;
       </td>
       <td class="tableData">
          &nbsp;
       </td>
   </tr>
<tmpl_else>
   <tmpl_loop messages>
      <tr>
         <td class="tableData">
            <tmpl_var subject>
         </td>
         <td class="tableData">
            <tmpl_var status>
         </td>
         <td class="tableData">
            <tmpl_var dateStamp>
         </td>
     </tr>
  </tmpl_loop>
</tmpl_if>
</table>
   <ul class="accountOptions">
      <tmpl_loop accountOptions>
         <li><tmpl_var options.display>
      </tmpl_loop>
   </ul>
