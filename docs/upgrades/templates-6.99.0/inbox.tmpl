<h1><tmpl_var title></h1>

<table width="100%" cellspacing="1" cellpadding="2" border="0">
<tr>
   <td class="tableHeader">
      <tmpl_var subject.label>
   </td>
   <td class="tableHeader">
      <tmpl_var status.label>
   </td>
   <td class="tableHeader">
      <tmpl_var dateStamp.label>
   </td>
</tr>
<tmpl_if message.noresults>
